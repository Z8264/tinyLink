{
  "name": "links",
  "version": "0.1",
  "manifest_version": 2,
  "description": "生厂力工具",
  "icons": {
    "128": "1.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["bg.js"]
  },
  "permissions": ["http://*.google.com/", "https://*.google.com/"],
  "browser_action": {
    "default_title": "",
    "default_icon": "1.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
